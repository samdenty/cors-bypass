{
  "name": "cors-bypass",
  "version": "1.3.0",
  "homepage": "https://github.com/samdenty/cors-bypass",
  "description": "Bypass the browsers CORS restrictions, without needing to setup a server-side proxy. ",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "tsc": "rm -rf lib; tsc",
    "watch": "yarn tsc -w",
    "webpack": "webpack --config webpack.config.js",
    "start": "parcel serve demo/**/*.html",
    "demo": "parcel build demo/**/*.html",
    "build": "yarn tsc; yarn webpack"
  },
  "sideEffects": false,
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/event-emitter": "^0.3.3",
    "event-emitter": "^0.3.5",
    "event-target-shim": "^4.0.3"
  },
  "devDependencies": {
    "mqtt": "^2.18.8",
    "parcel-bundler": "^1.11.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1"
  }
}
